{
  "name": "agenda4pets",
  "version": "0.0.1",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start ",
    "deploy:local:windows": "set APP_ENV=DEV&& expo start",
    "deploy:local": "APP_ENV=DEV expo start ",
    "publish:production:windows": "set APP_ENV=PRD&&expo publish --release-channel production",
    "publish:staging:windows": "set APP_ENV=DEV&&expo publish --release-channel staging",
    "publish:homolog:windows": "set APP_ENV=HOM&&expo publish --release-channel homolog",
    "deploy:production": "eas build --profile production",
    "deploy:homolog": "eas build --profile homolog",
    "deploy:staging": "eas build --profile staging",
    "debug": "react-devtools",
    "eject": "expo eject",
    "test": "jest",
    "storybook": "start-storybook"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "6.0.0",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "@react-navigation/stack": "^6.1.1",
    "async-storage": "^0.1.0",
    "axios": "^0.20.0",
    "expo": "~41.0.0",
    "expo-dev-client": "~0.6.3",
    "expo-linear-gradient": "~9.1.0",
    "expo-status-bar": "~1.0.4",
    "expo-updates": "~0.5.4",
    "react": "~16.11.0",
    "react-dom": "~16.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-41.0.0.tar.gz",
    "react-native-animatable": "^1.3.3",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-safe-area-context": "3.2.0",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "~3.0.0",
    "react-native-snap-carousel": "^3.9.0",
    "react-native-web": "~0.11.7",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/runtime": "^7.9.6",
    "@react-native-community/eslint-config": "^1.1.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-ondevice-actions": "^5.3.19",
    "@storybook/addons": "^6.0.16",
    "@storybook/react-native": "^5.3.19",
    "@storybook/react-native-server": "^5.3.19",
    "@storybook/theming": "^6.0.16",
    "@testing-library/react-native": "^7.0.1",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-preset-expo": "~8.1.0",
    "eslint": "^7.0.0",
    "jest": "^26.0.1",
    "jest-expo": "^37.0.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.11.0"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__mocks__/globalMock.js"
    ]
  },
  "private": true,
  "prettier": {
    "endOfLine": "auto",
    "singleQuote": true
  }
}
