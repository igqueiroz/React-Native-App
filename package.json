{
  "name": "expo-app",
  "version": "0.0.1",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "cross-env APP_ENV=DEV expo start",
    "deploy:local": "cross-env APP_ENV=DEV expo start ",
    "publish:production": "cross-env APP_ENV=PRD expo publish --release-channel production",
    "publish:staging": "cross-env APP_ENV=DEV expo publish --release-channel staging",
    "publish:homolog": "cross-env APP_ENV=HOM expo publish --release-channel homolog",
    "deploy:production": "cross-env APP_ENV=PRD eas build --profile production",
    "deploy:homolog": "eas build --profile homolog",
    "deploy:staging": "eas build --profile staging",
    "eject": "expo eject",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "7.1.3",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "@react-navigation/stack": "^6.1.1",
    "async-storage": "^0.1.0",
    "axios": "^0.26.0",
    "expo": "~44.0.0",
    "expo-linear-gradient": "~11.0.3",
    "expo-status-bar": "~1.2.0",
    "expo-updates": "~0.11.6",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-masked-text": "^1.13.0",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-web": "0.17.1",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3"
  },
  "private": true,
  "prettier": {
    "endOfLine": "auto",
    "singleQuote": true
  }
